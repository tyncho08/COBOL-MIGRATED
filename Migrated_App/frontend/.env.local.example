# ACAS Migrated Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_VERSION=v1

# Application Information
NEXT_PUBLIC_APP_NAME="ACAS Migrated"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_COMPANY_NAME="Your Company Name"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true
NEXT_PUBLIC_ENABLE_DEMO_MODE=false

# Date/Time Formats
NEXT_PUBLIC_DATE_FORMAT="MM/DD/YYYY"
NEXT_PUBLIC_TIME_FORMAT="12h"
NEXT_PUBLIC_CURRENCY_SYMBOL="$"
NEXT_PUBLIC_CURRENCY_CODE="USD"

# UI Configuration
NEXT_PUBLIC_ITEMS_PER_PAGE=20
NEXT_PUBLIC_MAX_UPLOAD_SIZE=52428800  # 50MB in bytes
NEXT_PUBLIC_SESSION_TIMEOUT=30  # minutes

# External Services (if any)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=""
NEXT_PUBLIC_SENTRY_DSN=""